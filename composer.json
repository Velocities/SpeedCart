{
    "name": "velocities/linode_deployment",
    "description": "A brief description of your project",
    "type": "project",
    "require": {
        "php": "^8.1.25",
        "kreait/firebase-php": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10"
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
